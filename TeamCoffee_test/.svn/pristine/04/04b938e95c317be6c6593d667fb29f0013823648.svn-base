package com.teamcoffee.menu.vo;

public class MenuVO {
	// 메뉴 코드 
    private String menuCode;

    // 카페 코드 
    private String cafeCode;

    // 메뉴 카테고리 코드 
    private String menuCategoryCode;

    // 메뉴 이름 
    private String menuName;

    // 메뉴 가격 
    private int menuPrice;

    // 메뉴 설명 
    private String menuInfo;

    // 메뉴 상태 
    private String menuStatus;

    // 추가 옵션 코드 
    private String menuOptionCode;

    public String getMenuCode() {
        return menuCode;
    }

    public void setMenuCode(String menuCode) {
        this.menuCode = menuCode;
    }

    public String getCafeCode() {
        return cafeCode;
    }

    public void setCafeCode(String cafeCode) {
        this.cafeCode = cafeCode;
    }

    public String getMenuCategoryCode() {
        return menuCategoryCode;
    }

    public void setMenuCategoryCode(String menuCategoryCode) {
        this.menuCategoryCode = menuCategoryCode;
    }

    public String getMenuName() {
        return menuName;
    }

    public void setMenuName(String menuName) {
        this.menuName = menuName;
    }

    public int getMenuPrice() {
        return menuPrice;
    }

    public void setMenuPrice(int menuPrice) {
        this.menuPrice = menuPrice;
    }

    public String getMenuInfo() {
        return menuInfo;
    }

    public void setMenuInfo(String menuInfo) {
        this.menuInfo = menuInfo;
    }

    public String getMenuStatus() {
        return menuStatus;
    }

    public void setMenuStatus(String menuStatus) {
        this.menuStatus = menuStatus;
    }

    public String getMenuOptionCode() {
        return menuOptionCode;
    }

    public void setMenuOptionCode(String menuOptionCode) {
        this.menuOptionCode = menuOptionCode;
    }

    // DjvMenu 모델 복사
    public void CopyData(MenuVO param)
    {
        this.menuCode = param.getMenuCode();
        this.cafeCode = param.getCafeCode();
        this.menuCategoryCode = param.getMenuCategoryCode();
        this.menuName = param.getMenuName();
        this.menuPrice = param.getMenuPrice();
        this.menuInfo = param.getMenuInfo();
        this.menuStatus = param.getMenuStatus();
        this.menuOptionCode = param.getMenuOptionCode();
    }
}
