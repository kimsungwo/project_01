package com.teamcoffee.member.vo;

//관심목록 
public class WishVO {

 // 관심코드 
 private String wishCode;

 // 회원코드 
 private String memCode;

 // 체인점코드 
 private String chainCode;

 public String getWishCode() {
     return wishCode;
 }

 public void setWishCode(String wishCode) {
     this.wishCode = wishCode;
 }

 public String getMemCode() {
     return memCode;
 }

 public void setMemCode(String memCode) {
     this.memCode = memCode;
 }

 public String getChainCode() {
     return chainCode;
 }

 public void setChainCode(String chainCode) {
     this.chainCode = chainCode;
 }

 // DjvWish 모델 복사
 public void CopyData(WishVO param)
 {
     this.wishCode = param.getWishCode();
     this.memCode = param.getMemCode();
     this.chainCode = param.getChainCode();
 }
}