package com.teamcoffee.menu.vo;

public class MenuCategoryVO {
    // 메뉴 카테고리 코드 
    private String menuCategoryCode;

    // 카테고리 이름 
    private String categoryName;
    
    // 카페코드 
    private String cafeCode;

    public String getMenuCategoryCode() {
        return menuCategoryCode;
    }

    public void setMenuCategoryCode(String menuCategoryCode) {
        this.menuCategoryCode = menuCategoryCode;
    }

    public String getCategoryName() {
        return categoryName;
    }

    public void setCategoryName(String categoryName) {
        this.categoryName = categoryName;
    }

    public String getCafeCode() {
        return cafeCode;
    }

    public void setCafeCode(String cafeCode) {
        this.cafeCode = cafeCode;
    }

    // DjvMenuCategory 모델 복사
    public void CopyData(MenuCategoryVO param)
    {
        this.menuCategoryCode = param.getMenuCategoryCode();
        this.categoryName = param.getCategoryName();
        this.cafeCode = param.getCafeCode();
    }

}
